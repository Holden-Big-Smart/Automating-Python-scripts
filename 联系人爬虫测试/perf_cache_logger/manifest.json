{
  "manifest_version": 3,
  "name": "XHR JSON 请求文件名打印器",
  "version": "1.0",
  "description": "在控制台打印所有 xhr/json 请求的逻辑文件名",
  "permissions": ["webRequest", "webRequestBlocking", "<all_urls>"],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
