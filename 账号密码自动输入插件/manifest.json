{
  "manifest_version": 3,
  "name": "账密自动输入",
  "version": "1.0",
  "description": "在页面中自动填入账号和密码",
  "permissions": ["scripting", "activeTab"],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ]
}
